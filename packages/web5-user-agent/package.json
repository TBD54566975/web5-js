    "karma": "6.4.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-esbuild": "2.2.5",
    "karma-firefox-launcher": "2.1.2",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-webkit-launcher": "2.1.0",
    "mocha": "10.2.0",
    "node-stdlib-browser": "1.2.0",
    "rimraf": "4.4.0",
    "sinon": "15.0.2",
    "source-map-loader": "4.0.1",
    "typescript": "5.0.4"
  },
  "overrides": {
    "socket.io-parser@>4.0.4 <4.2.3": "4.2.3"
  }
